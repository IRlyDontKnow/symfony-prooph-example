{
    "name": "zawiszaty/php-prooph-example",
    "description": "prooph example in symfony",
    "authors": [
        {
            "name": "zawiszaty",
            "email": "szymonciom@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"App\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "prooph/service-bus": "^6.2",
        "prooph/event-sourcing": "^5.6",
        "prooph/event-store": "^7.5",
        "prooph/pdo-event-store": "^1.10",
        "prooph/event-store-bus-bridge": "^3.3",
        "prooph/snapshotter": "^2.1",
        "prooph/pdo-snapshot-store": "^1.5",
        "symfony/dependency-injection": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/config": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/framework-bundle": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.11.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "sensiolabs-de/deptrac-shim": "^0.5.0"
    }
}
